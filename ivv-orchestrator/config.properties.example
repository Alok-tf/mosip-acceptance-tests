ivv.mosip.host=https://int.mosip.io
ivv.prereg.version=v1
ivv.prereg.apiversion=1.0
ivv.global.version=v1
ivv.global.apiversion=1.0

# Dependency server info #
ivv.swagger.host=http://localhost:8081

# email account #
ivv.email.server.host=
ivv.email.server.user=
ivv.email.server.pass=

# text for finding otp message in mail
ivv.prereg.otp.subject=Otp message

# skip id fields in prereg, regclient, ida
ivv.prereg.skip=age,IDSchemaVersion,UIN,localAdministrativeAuthority,parentOrGuardianName,parentOrGuardianRID,parentOrGuardianUIN,CNIENumber
ivv.regclient.skip=

