#ivv.sheet.scenario.sheet=local/ScenarioSheetFull.csv
ivv.path.scenario.sheet=local/ScenarioSheetPrereg.csv
ivv.path.globals.sheet=local/GlobalsSheet.csv
ivv.path.configs.sheet=local/ConfigsSheet.csv
ivv.path.documents.sheet=local/DocumentsSheet.csv
ivv.path.biometrics.sheet=local/BiometricsSheet.csv
ivv.path.documents.folder=local/documents/
ivv.path.biometrics.folder=local/biometrics/
ivv._path.auditlog=testRun/reports/mosip_auditLogs.log
ivv._path.reports=testRun/reports/mosip_reports.html
ivv.path.mappingfile=local/mapping.json
ivv.path.idobject=local/IDObjectSchema.json